{
  "name": "wuns",
  "version": "0.0.2",
  "description": "the wuns programming language extension",
  "engines": {
    "vscode": "^1.88.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "wunslang.check",
        "title": "Check current file"
      },
      {
        "command": "wunslang.interpret",
        "title": "Interpret current file"
      }
    ],
    "languages": [
      {
        "id": "wuns",
        "extensions": [
          ".wuns"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wuns",
        "scopeName": "source.wuns",
        "path": "./wuns.tmGrammar.json"
      }
    ]
  },
  "main": "./vscode-extension.js",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false,
    "printWidth": 120
  },
  "dependencies": {
    "tree-sitter": "^0.21.1",
    "tree-sitter-wuns": "file:tree-sitter-wuns/tree-sitter-wuns-1.0.2.tgz",
    "wabt": "^1.0.35"
  }
}
